        "file_number": "file\\s+no\\s+\\(\\d+\\):\\s+([\\d\\w\\/\\s]+)(?=mark\\sname)",
        "mark_name": "mark\\s+name:\\s+([\\w\\d\\-\\.\\s])+(?=applicant)",
